<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="idb_sample_score_form" model="ir.ui.view">
            <field name="name">Sample rating sheet</field>
            <field name="model">idb.sample.score</field>
            <field name="arch" type="xml">
                <form string="Sample rating sheet">
                    <group>
<!--                        <field name="score"/>-->
